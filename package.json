{
    "name": "buldak",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,json,js}\"",
        "new-service": "yarn hygen service new"
    },
    "devDependencies": {
        "@packages/eslint-config-custom": "*",
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "dependencies": {
        "hygen": "^6.2.7",
        "react-reveal": "^1.2.2"
    },
    "resolutions": {
        "@types/react": "17.0.2",
        "@types/react-dom": "17.0.2"
    },
    "packageManager": "yarn@1.22.18"
}
